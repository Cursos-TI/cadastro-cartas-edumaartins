#include <stdio.h>

int main (){

    printf("desafio cartas super trunfo!\n");
    
    printf("Primeira Carta:\n");
    char Estado1 []  = "Rio Grande do Sul";

    char codigo_carta1 [] = "A01";

    char cidade1 [] = "Porto Alegre";

    float  populacao1 = 1388.794;

    float area1 = 496.8; 

    float pib = 81.56;

    int pontos_turisticos1 = 38;
     
    printf("Estado: %s\n", Estado1);
    printf("Codigo da carta: %s\n", codigo_carta1);
    printf("Cidade: %s\n", cidade1);
    printf("Populacao: %f\n", populacao1);
    printf("Area: %f\n", area1);
    printf("PIB: %f\n", pib);
    printf("Pontos Turisticos: %d\n", pontos_turisticos1); 
    

    printf("Segunda Carta:\n");
    char estado2[] = "Rio de Janeiro";

    char codigo_carta2 [] = "B02"; 

    char cidade2[] = "Angra dos Reis";

    float populacao2 = 179.120;

    float area2 = 813.420;

    float pib2 = 11.194;

    int pontos_turisticos2 = 10;  

    printf("Estado: %s\n", estado2);
    printf("Codigo da carta: %s\n", codigo_carta2);
    printf("Cidade: %s\n", cidade2);
    printf("Populacao: %f\n", populacao2);
    printf("Area: %f\n", area2);
    printf("PIB: %f\n", pib);
    printf("Pontos Turisticos: %d\n", pontos_turisticos2);

    
    return 0;
}
